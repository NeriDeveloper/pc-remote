{
  "name": "remote-control-server",
  "productName": "PC Remote Server",
  "description": "HTTP remote control server with socket.io",
  "version": "0.0.1",
  "author": "amoshydra",
  "private": true,
  "postinstall": "install-app-deps",
  "scripts": {
    "start": "electron ./application/main.js",
    "startServer": "node ./server/bin/www",
    "rebuild": "npm rebuild --runtime=electron --target=1.4.13 --disturl=https://atom.io/download/atom-shell --abi=48"
  },
  "build": {
    "appId": "com.amoshydra.electron.youtubetv"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "hbs": "~4.0.0",
    "lodash": "^4.17.2",
    "morgan": "~1.7.0",
    "robotjs": "^0.4.5",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.7.2",
    "uws": "^0.12.0"
  },
  "devDependencies": {
    "7zip-bin-win": "^2.0.2",
    "electron": "1.4.13",
    "electron-builder": "^10.9.2"
  }
}
